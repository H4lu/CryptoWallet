{
  "name": "CryptoWallet",
  "version": "1.1.6a",
  "description": "CET CryptoWallet application",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "rebuild": "electron-rebuild",
    "make-release": "electron-forge make --skip-package",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "H4lu",
    "email": "halurecht@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "asar": false
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "CryptoWallet_Kovan"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "H4lu",
              "name": "CryptoWallet"
            }
          },
          "prerelease": true
        }
      ],
      "hooks": "require:./hooks.js",
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.57",
    "@electron-forge/maker-deb": "^6.0.0-beta.57",
    "@electron-forge/maker-rpm": "^6.0.0-beta.57",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.57",
    "@electron-forge/maker-zip": "^6.0.0-beta.57",
    "@electron-forge/plugin-auto-unpack-natives": "^6.0.0-beta.57",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.57",
    "@electron-forge/publisher-github": "^6.0.0-beta.57",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/ffi-napi": "^4.0.1",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@types/react-router": "^5.1.14",
    "@types/react-router-dom": "^5.1.7",
    "@types/recharts": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.6",
    "electron": "13.1.1",
    "eslint": "^7.28.0",
    "eslint-plugin-import": "^2.23.4",
    "file-loader": "1.1.11",
    "fork-ts-checker-webpack-plugin": "^6.2.10",
    "node-loader": "^2.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@ethereumjs/tx": "^3.2.0",
    "@pokusew/pcsclite": "0.6.0",
    "@types/bitcoinjs-lib": "3.0.5",
    "assert": "^2.0.0",
    "axios": "^0.21.1",
    "bitcoinjs-lib": "3.3.0",
    "electron-squirrel-startup": "^1.0.0",
    "ffi-napi": "^4.0.3",
    "html-webpack-plugin": "^5.3.1",
    "js-sha3": "0.8.0",
    "react": "17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "recharts": "^2.0.9",
    "rlp": "2.0.0",
    "satoshi-bitcoin": "1.0.4",
    "utf-8-validate": "^5.0.5",
    "web3": "^1.3.6"
  }
}
